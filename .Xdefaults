! Term env var and login shell {{{1
*termName:                         xterm-256color
*loginShell:                       true

! Scrolls {{{1
! I want to scroll if I type something, jump (fast scrolling)
*scrollTtyKeypress:                true
*jumpScroll:                       true
! Disable everything else
*scrollstyle:                      plain
*scrollTtyOutput:                  false
*scrollWithBuffer:                 false
*skipScroll:                       false
*scrollBar:                        false
*scrollBar_right:                  false
*scrollBar_floating:               false
*saveLines:                        0
*thickness:                        0

! Secondary screen and scroll if so {{{1
*secondaryScreen:                  true
*secondaryScroll:                  true

! use the fuckin bell :P {{{1
*visualBell:                       true
*mapAlert:                         true
*urgentOnBell:                     true

! Colors {{{1

! DONT FUCKIN SET THESE!!
! *highlightColor:                 BanishYouToTheShadowRealmIfYouSetThis
! *highlightTextColor:             BanishYouToTheShadowRealmIfYouSetThis
! *cursorColor:       #ffffff
! *altIsNotMeta:false
xterm*metaSendsEscape:  true
!*cursorColor2:      #ffffff
! Mappings {{{1
.iso14755:                         false
.iso14755_52:                      false
.keysym.M-s:                       builtin-string:
.keysym.M-S:                       builtin-string:
.keysym.C-s-t:                     builtin-string:

.keysym.C-S-0x43:                  eval:selection_to_clipboard
.keysym.C-S-0x56:                  eval:paste_primary
.tripleclickwords:                 true
xterm*VT100.Translations: #override \
    Ctrl Meta <Key>V: insert-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>C:    copy-selection(CLIPBOARD) \n\
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(d)

! Fonty Python {{{1
*font:                               xft:Pt Mono:size=11
xterm*faceName: PT Mono
xterm*faceSize: 11
xterm*renderFont: true
