! Term env var and login shell {{{1
*termName:                         xterm-256color
*loginShell:                       true
urxvt*depth: 32

! Scrolls {{{1
*scrollTtyKeypress:                true
*jumpScroll:                       true
*scrollstyle:                      plain
*scrollTtyOutput:                  false
*scrollWithBuffer:                 false
*skipScroll:                       false
*scrollBar:                        false
*scrollBar_right:                  false
*scrollBar_floating:               false
*saveLines:                        0
*thickness:                        0

! Secondary screen and scroll if so {{{1
*secondaryScreen:                  true
*secondaryScroll:                  true

! use the fuckin bell :P {{{1
*visualBell:                       true
*mapAlert:                         true
*urgentOnBell:                     true

! Colors {{{1
URxvt.perl-ext:           default
URxvt.url-launcher:       sensible-browser

! *highlightColor:                 BanishYouToTheShadowRealmIfYouSetThis
! *highlightTextColor:             BanishYouToTheShadowRealmIfYouSetThis
! *altIsNotMeta:false
xterm*metaSendsEscape:  true
!*cursorColor2:      #ffffff
! Mappings {{{1
Rxvt.iso14755         : false
Rxvt.iso14755_52      : false
Rxvt.keysym.M-s       : builtin-string:
Rxvt.keysym.M-S       : builtin-string:
Rxvt.keysym.C-s-t     : builtin-string:

Rxvt.keysym.C-S-0x43  : eval:selection_to_clipboard
Rxvt.keysym.C-S-0x56  : eval:paste_primary
Rxvt.tripleclickwords : true

URxvt.iconFile        : /home/brcoffman/Pictures/cheetah.jpg

! Fonty Python {{{1
*font:                               xft:Pt Mono:size=13
xterm*faceName: PT Mono
xterm*faceSize: 9
xterm*renderFont: true
